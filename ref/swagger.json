{
    "swagger": "2.0",
    "info": {
        "version": "1.0",
        "title": "Swagger boilerplate",
        "description": "스웨거 샘플"
    },
    "host": "localhost:10080",
    "basePath": "/api",
    "schemes": [
        "http",
        "https"
    ],
    "paths": {
        "/human": {
            "post": {
                "description": "Creates a new human in the list",
                "operationId": "addHuman",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "human",
                        "in": "body",
                        "description": "인명정보",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/NewHuman"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Human 응답",
                        "schema": {
                            "$ref": "#/definitions/Human"
                        }
                    },
                    "default": {
                        "description": "기본 오류 : 미지정된 오류",
                        "schema": {
                            "$ref": "#/definitions/ErrorModel"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "서버 실행 여부 체크",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/": {
            "get": {
                "description": "루트",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "definitions": {
        "Human": {
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/NewHuman"
                },
                {
                    "required": [
                        "name"
                    ]
                }
            ]
        },
        "NewHuman": {
            "type": "object",
            "required": [
                "name", "gender", "age"
            ],
            "properties": {
                "name": {
                    "type": "string"
                },
                "gender": {
                    "type": "string"
                },
                "age": {
                    "type": "integer",
                    "format": "int64"
                }
            }
        },
        "ErrorModel": {
            "type": "object",
            "required": [
                "code",
                "message"
            ],
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
}